{
  "name": "sparkx-express-starter",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/SparkxFoundation/sparkx-express-starter.git",
  "author": "DangaRanga <jasonblue1626@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "14.17.6"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "eslint": "^7.29.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.0.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1",
    "supertest": "^6.1.3",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev-start": "nodemon --exec babel-node src/index.js",
    "build": "npx sequelize-cli db:migrate",
    "lint": "eslint ./src",
    "test": "jest",
    "coverage": "jest --coverage",
    "migrate": "npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "undo": "npx sequelize-cli db:migrate:undo",
    "undo-all": "npx sequelize-cli db:migrate:undo:all",
    "new": "npx sequelize-cli migration:generate",
    "format": "prettier --write \"**/*.{js,jsx,json,md,scss,css}\"",
    "lint-fix": "eslint ./src --fix"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jquery-csv": "^1.0.21",
    "jsonwebtoken": "^8.5.1",
    "mailgen": "^2.0.20",
    "nodemailer": "^6.7.3",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "postgres": "^3.0.0",
    "sequelize": "^6.17.0",
    "sequelize-cli": "^6.4.1"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
